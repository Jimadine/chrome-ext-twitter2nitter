{
  "manifest_version": 3,
  "name": "Twitter2Nitter",
  "author": "Jim Adamson",
  "version": "0.0.1",
  "description": "Redirect from Twitter to Nitter",
  "minimum_chrome_version": "121",
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  },
  "background": {
    "scripts": ["scripts/service_worker.js"],
    "service_worker": "scripts/service_worker.js"
  },
  "icons": {
   "48": "icons/icon48.png",
   "128": "icons/icon128.png"
 },
  "permissions": [
    "storage",
    "webNavigation"
  ]
}
